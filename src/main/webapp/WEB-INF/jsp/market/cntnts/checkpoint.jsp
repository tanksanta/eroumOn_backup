        <script>
            $(function() {
                if (window.location.hash) {
                    var hash = "#" + window.location.hash.replace(/^#/,'');
                    if($(hash).length > 0) {
                        var navigation = $('.checkpoint-navigation .container');
                        var container  = $('.checkpoint-container');

                        $('a[class*="nav-item"][href="' + hash + '"]').addClass('active').parent().siblings().each(function() {
                            $(this).find('a[class*="nav-item"]').removeClass('active');
                        });

                        $('.tab-pane').removeClass('show active').filter(hash).addClass('show active');
                        $(window).scrollTop(0).one('load', function() {
                        	$(this).scrollTop((container.offset().top - parseInt(container.css('margin-top').replace('px', ''))) - ($('#header').outerHeight() + navigation.outerHeight()));
                        })

                        navigation.scrollLeft(navigation.scrollLeft() + ((navigation.width() - ($('a[class*="nav-item"][href="' + hash + '"]').offset().left + $('a[class*="nav-item"][href="' + hash + '"]').width())) * -1));
                    }
                } else {
                    $('a[class*="nav-item"][href="#checkpoint-content3"]').addClass('active');
                    $('.tab-pane').removeClass('show active').filter('#checkpoint-content3').addClass('show active');
                }

                horizonScroll($('.checkpoint-navigation .container'));

                $('[class*="nav-item"]').on('click', function() {
                    var navigation = $('.checkpoint-navigation .container');
                    var container  = $('.checkpoint-container');

                    $($(this).attr('href')).one('transitionend', function() {
                        $(window).scrollTop((container.offset().top - parseInt(container.css('margin-top').replace('px', ''))) - ($('#header').outerHeight() + navigation.outerHeight()));
                    })
                });

                $('.checkpoint-img-content .tab a').on('click', function() {
                    $($(this).attr('href')).addClass('block').removeClass('hidden').siblings('.checkpoint-img-content').addClass('hidden').removeClass('block');
                    return false;
                });
            })
        </script>
